{
    "Lua.runtime.version": "Lua 5.1",
    "Lua.runtime.path": [
        "?.lua",
        "?/init.lua",
        "/?"
    ],
    "Lua.completion.showWord": "Disable",
    "Lua.runtime.special": {
        "import": "require",
        "doscript": "require"
    },
    "Lua.runtime.nonstandardSymbol": [
        "!="
    ],
    "Lua.completion.requireSeparator": "/",
    "Lua.runtime.exportEnvDefault": true,
    "Lua.diagnostics.globals": [
        "moho"
    ],
    "Lua.runtime.plugin": "${3rd}/fa/plugin.lua",
    "Lua.workspace.library": [
        "${3rd}/fa/library"
    ],
    "Lua.workspace.checkThirdParty": false,

    "workbench.colorCustomizations": {
        "activityBar.background": "#324376",
        "activityBar.foreground": "#F5DD90",
        "statusBar.background": "#324376",
        "statusBar.foreground": "#F5DD90",
        "titleBar.activeBackground": "#324376",
        "titleBar.activeForeground": "#F5DD90",
        "titleBar.inactiveBackground": "#324376",
        "titleBar.inactiveForeground": "#F5DD90"
    }
}